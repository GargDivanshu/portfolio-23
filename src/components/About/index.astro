          </div>
        </div>
        <div class="flex flex-col md:w-1/3 w-full text-left text-[0.85rem] uppercase text-white">
          Frameworks / Technologies
          <div class="flex flex-wrap gap-2 py-4">
            <p class="border-[1.5px] border-white/80 hover:border-white rounded-[0.6rem] p-[0.6rem] text-[0.7rem] uppercase text-white/80 hover:text-white duration-200 ease-in-out">React</p> 
            <p class="border-[1.5px] border-white/80 hover:border-white rounded-[0.6rem] p-[0.6rem] text-[0.7rem] uppercase text-white/80 hover:text-white duration-200 ease-in-out">NextJS</p> 
            <p class="border-[1.5px] border-white/80 hover:border-white rounded-[0.6rem] p-[0.6rem] text-[0.7rem] uppercase text-white/80 hover:text-white duration-200 ease-in-out">MongoDB</p>
            <p class="border-[1.5px] border-white/80 hover:border-white rounded-[0.6rem] p-[0.6rem] text-[0.7rem] uppercase text-white/80 hover:text-white duration-200 ease-in-out">PostgresQL</p> 
            <p class="border-[1.5px] border-white/80 hover:border-white rounded-[0.6rem] p-[0.6rem] text-[0.7rem] uppercase text-white/80 hover:text-white duration-200 ease-in-out">GraphQL</p>
            <p class="border-[1.5px] border-white/80 hover:border-white rounded-[0.6rem] p-[0.6rem] text-[0.7rem] uppercase text-white/80 hover:text-white duration-200 ease-in-out">Google Cloud</p> 
            <p class="border-[1.5px] border-white/80 hover:border-white rounded-[0.6rem] p-[0.6rem] text-[0.7rem] uppercase text-white/80 hover:text-white duration-200 ease-in-out">Webhooks</p>
            <p class="border-[1.5px] border-white/80 hover:border-white rounded-[0.6rem] p-[0.6rem] text-[0.7rem] uppercase text-white/80 hover:text-white duration-200 ease-in-out">Redis</p>
            <p class="border-[1.5px] border-white/80 hover:border-white rounded-[0.6rem] p-[0.6rem] text-[0.7rem] uppercase text-white/80 hover:text-white duration-200 ease-in-out">PusherJS</p>
          </div>
        </div>
        <div class="flex flex-col md:w-1/3 w-full text-left text-[0.85rem] uppercase text-white">
          Stuff I'm Learning
          <div class="flex flex-wrap gap-2 py-4">
            <p class="border-[1.5px] border-white/80 hover:border-white rounded-[0.6rem] p-[0.6rem] text-[0.7rem] uppercase text-white/80 hover:text-white duration-200 ease-in-out">Rust</p>
            <p class="border-[1.5px] border-white/80 hover:border-white rounded-[0.6rem] p-[0.6rem] text-[0.7rem] uppercase text-white/80 hover:text-white duration-200 ease-in-out">Tokio</p>
            <p class="border-[1.5px] border-white/80 hover:border-white rounded-[0.6rem] p-[0.6rem] text-[0.7rem] uppercase text-white/80 hover:text-white duration-200 ease-in-out">Java</p>           
            <p class="border-[1.5px] border-white/80 hover:border-white rounded-[0.6rem] p-[0.6rem] text-[0.7rem] uppercase text-white/80 hover:text-white duration-200 ease-in-out">Spring Boot</p> 
            <p class="border-[1.5px] border-white/80 hover:border-white rounded-[0.6rem] p-[0.6rem] text-[0.7rem] uppercase text-white/80 hover:text-white duration-200 ease-in-out">Spring Security</p> 
          </div>
        </div>
      </div>
    </div>
  </div>
  </div>


  <script is:inline >
    import { annotate } from 'rough-notation';

    const freelance = annotate(document.querySelector('highlight'), {type: 'highlight', color: '#F59E0B'});
    freelance.show();
  </script>

  

<script>
  import gsap from 'gsap'
  import SplitType from 'split-type';
  gsap.registerPlugin(ScrollTrigger);

  const splitTypes = document.querySelectorAll(".revealType");
  splitTypes.forEach((char) => {
    // @ts-ignore
    const text = new SplitType(char as HTMLElement, { types: '"words, chars"' });


    gsap.from(text.chars, {
      scrollTrigger: {
        trigger: char,
        start: "top 75%",
        end: "top 10%",
        scrub: true,
        markers: false,
        toggleActions: "play play reverse reverse",
      },
      opacity: 0.2,
      stagger: 0.1,
    });
  });
  </script>